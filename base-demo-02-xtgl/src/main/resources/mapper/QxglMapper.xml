<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.basedemo02xtgl.basedemo02xtgl.dao.QxglMapper">
    <resultMap id="SysMenuResultMap" type="com.basedemo02xtgl.basedemo02xtgl.entity.SysMenu">
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="parent_id" property="parentId" jdbcType="INTEGER" />
        <result column="name" property="name" jdbcType="VARCHAR" />
        <result column="perms" property="perms" jdbcType="VARCHAR" />
        <result column="type" property="type" jdbcType="INTEGER" />
        <result column="icon" property="icon" jdbcType="VARCHAR" />
        <result column="created" property="created" jdbcType="VARCHAR" />
        <result column="updated" property="updated" jdbcType="VARCHAR" />
        <result column="state" property="state" jdbcType="INTEGER" />
    </resultMap>

    <sql id="field">
        id, parent_id as parentId, name, perms, type, date_format(created, '%Y-%c-%d %h:%i:%s') as created, date_format(updated, '%Y-%c-%d %h:%i:%s' ) as updated, state
    </sql>
    <select id="queryMenus" resultMap="SysMenuResultMap">
        select <include refid="field" /> from sys_menu
    </select>

</mapper>
